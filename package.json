{
  "private": true,
  "name": "buom01_tremulous",
  "version": "0.0.0",
  "scripts": {
    "build": "preact build --preload --brotli --gzip",
    "serve": "sirv build --port 8080 --cors --single --brotli --gzip",
    "dev": "preact watch",
    "lint": "eslint src",
    "test": "jest"
  },
  "eslintConfig": {
    "extends": "preact",
    "ignorePatterns": [
      "build/"
    ]
  },
  "devDependencies": {
    "augmented-ui": "^1.1.2",
    "classnames": "^2.2.6",
    "cssnano-preset-lite": "^1.0.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-preact-pure": "^2.0.0",
    "eslint": "^6.0.1",
    "eslint-config-preact": "^1.1.0",
    "grd": "git+https://github.com/paaragon/grd.git",
    "jest": "^24.9.0",
    "jest-preset-preact": "^1.0.0",
    "less-loader": "^6.2.0",
    "normalize.css": "^8.0.1",
    "preact-cli": "^3.0.0",
    "react-waypoint": "^9.0.3",
    "sass": "^1.26.10",
    "sass-loader": "^9.0.3",
    "sirv-cli": "1.0.3"
  },
  "dependencies": {
    "preact": "^10.3.2",
    "preact-render-to-string": "^5.1.4",
    "preact-router": "^3.2.1"
  },
  "jest": {
    "preset": "jest-preset-preact",
    "setupFiles": [
      "<rootDir>/tests/__mocks__/browserMocks.js",
      "<rootDir>/tests/__mocks__/setupTests.js"
    ]
  },
  "cssnano": {
    "preset": [
      "default",
      {
        "calc": false,
        "normalizeWhitespace": false
      }
    ]
  }
}
