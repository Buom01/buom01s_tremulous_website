  "eslintConfig": {
    "extends": "preact",
    "ignorePatterns": [
      "build/"
    ]
  },
  "devDependencies": {
    "augmented-ui": "^1.1.2",
    "classnames": "^2.3.2",
    "cssnano-preset-lite": "^2.1.3",
    "enzyme": "^3.10.0",
    "enzyme-adapter-preact-pure": "^4.0.1",
    "eslint": "^8.27.0",
    "eslint-config-preact": "^1.3.0",
    "grd": "git+https://github.com/paaragon/grd.git",
    "jest": "^27.5.1",
    "jest-preset-preact": "^4.0.5",
    "less-loader": "^7.3.0",
    "normalize.css": "^8.0.1",
    "preact-cli": "^3.4.1",
    "react-waypoint": "^10.3.0",
    "sass": "^1.56.0",
    "sass-loader": "^10.3.1",
    "sirv-cli": "^2.0.2"
  },
  "dependencies": {
    "preact": "^10.11.2",
    "preact-render-to-string": "^5.2.6",
    "preact-router": "^4.1.0"
  },
  "jest": {
    "preset": "jest-preset-preact",
    "setupFiles": [
      "<rootDir>/tests/__mocks__/browserMocks.js",
      "<rootDir>/tests/__mocks__/setupTests.js"
    ]
  },
  "cssnano": {
